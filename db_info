CREATE SEQUENCE neoway_id_seq INCREMENT 1;

CREATE TABLE neoway_case_tbl(
cpf CHARACTER VARYING(80) NOT NULL,
PRIVATE INTEGER NOT NULL,
INCOMPLETO INTEGER NOT NULL,
data_ultima_compra DATE,
ticket_medio FLOAT,
ticket_ultima_compra FLOAT,
loja_mais_frequente CHARACTER VARYING(80),
loja_ultima_compra CHARACTER VARYING(80)
)

SELECT * FROM neoway_case_tbl
DELETE FROM neoway_case_tbl;